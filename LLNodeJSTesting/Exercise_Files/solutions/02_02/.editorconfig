# Normalizes editor configuration.
# http://editorconfig.org
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = LF
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
cpp_space_between_square_brackets = true


[*.html]
indent_size = 4
max_line_length = 80
